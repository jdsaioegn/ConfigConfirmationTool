<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <!--ユーザ設定値-->
  <appSettings>
    <!--「チェック対象ファイル.ini」読み込み対象パス（変更不要）-->
    <add key="InitialFilePath" value=".\チェック対象ファイル.ini" />
    <!--ログ出力パス（変更不要）-->
    <add key="LogFilePath" value=".\Log\Log.txt"/>
    <!--トレースモード（変更不要）-->
    <add key="TraceMode" value="OFF"/>
  </appSettings>
</configuration>